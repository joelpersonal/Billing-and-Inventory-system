{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/app",
      "dest": "/dist/index.html"
    },
    {
      "src": "/app/(.*)",
      "dest": "/dist/index.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/assets/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}